package com.ict.edu;

public class Ex09 {
	public static void main(String[] args) {
		
	// do ~ while문: while문과 같은 반복문
	// 형식: 초기식;
	//		 do {
	//			 실행문;
	//			 증감식;
	//			} while(조건식);
	
	// 0 ~ 10 까지 출력
		int k1 = 0;
		do {
			System.out.print(k1 + " ");
			k1++;
		} while (k1 < 11);
		System.out.println();
		
	// 0 ~ 10 까지 짝수만 출력
		
	// 0 ~ 10 누적합
		
	// 구구단 5단 출력
		
	}
}
