package com.ict.edu;

public class Ex06 {
	public static void main(String[] args) {
		
	// 다중 for문 : for문 안에 다른 for문이 존재하는 것
		for (int i = 1; i < 4; i++) {
			for (int j = 1; j < 6; j++) {
				System.out.println("i= " + i + ", j= " + j);
			}
		}
		
	// 구구단 2단부터 9단까지
		for (int i = 2; i < 10; i++) {
			for (int j = 1; j < 10; j++) {
				System.out.println(i + " X " + j + " = " + (i * j));
			}
		}
		
	}
}
